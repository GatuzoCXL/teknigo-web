{
  "name": "teknigo-testing-suite",
  "version": "1.0.0",
  "description": "Suite completa de testing para Teknigo",
  "scripts": {
    "test": "cd .. && npm test",
    "test:selenium": "python selenium/web_tests_edge_fixed.py",
    "test:locust": "locust -f locust/load_test_simple.py --host=http://localhost:3000 -u 15 -r 3 -t 45s --headless",
    "test:security": "python security/security_tests.py",
    "test:all": "python scripts/comprehensive_test_runner.py",
    "test:extreme": "python scripts/comprehensive_test_runner.py 50 120",
    "clean": "cleanup_project_final.bat"
  },
  "keywords": [
    "testing",
    "selenium",
    "locust",
    "security",
    "jest"
  ],
  "author": "Gasut",
  "license": "MIT"
}
